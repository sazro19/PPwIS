                  <Button fx:id="mobileButton" mnemonicParsing="false" text="Оплата услуг мобильной связи" />
                  <Button fx:id="internetButton" mnemonicParsing="false" text="Оплата интернет услуг" />
                  <Button fx:id="getCard" mnemonicParsing="false" text="Забрать карту" />
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
